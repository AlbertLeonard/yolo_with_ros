#!/usr/bin/env python

import rospy 
import std_msgs 

from msg_gateway.msg import Command_msgs

frame = 0

def callback(msg):
	frame = frame + 1
	print '%d'%frame
	




#Main 
rospy.init_node('command_control')

sub = rospy.Subscriber('command_control/raw', Command_msgs, callback )
#pub = rospy.Publisher('command_control/raw', Command_msgs, queue_size=10 )

rospy.spin()

